{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 1080,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true,
        "ip": "127.0.0.1",
        "ip": "0.0.0.0",
        "userLevel": 0
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "block"
      }
    ],
    "balancers": [],
    "settings": {
      "domainStrategy": "IPIfNonMatch",
      "rules": [],
      "balancers": [],
      "outbound": {}
    }
  },
  "dns": {
    "servers": [
      "localhost"
    ]
  },
  "api": {
    "services": [
      "HandlerService",
      "LoggerService",
      "StatsService"
    ],
    "tag": "api"
  },
  "stats": {},
  "policy": {},
  "reverse": {},
  "transport": {},
  "streamSettings": {
    "network": "tcp",
    "security": "tls",
    "tlsSettings": {
      "serverName": "xuanyuanlove.top",
      "allowInsecure": false,
      "alpn": ["http/1.1"],
      "certificates": [
        "/root/.acme.sh/xuanyuanlove.top_ecc/fullchain.cer",
        "/root/.acme.sh/xuanyuanlove.top_ecc/xuanyuanlove.top.key"
      ]
    }
  }
}
